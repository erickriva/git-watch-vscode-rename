{
  "name": "git-watch-vscode-rename",
  "publisher": "erickriva",
  "repository": {
    "type": "git",
    "url": "https://github.com/erickriva/git-watch-vscode-rename"
  },
  "displayName": "git-watch-vscode-rename",
  "description": "Automatically run 'git mv' command, when renaming files/folders in VS Code, just for occasions where only file/folder name case was changed.",
  "version": "1.0.0",
  "engines": {
    "vscode": "^1.41.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./out/extension.js",
  "contributes": {},
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./"
  },
  "devDependencies": {
    "@types/node": "^12.11.7",
    "@types/vscode": "^1.41.0",
    "tslint": "^5.20.0",
    "typescript": "~4.6.3"
  },
  "dependencies": {}
}
